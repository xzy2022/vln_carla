{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "CARLA UE4 (app.main)",
      "type": "python",
      "request": "launch",
      "module": "app.main",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [
        "--carla-version",
        "ue4"
      ]
    },
    {
      "name": "CARLA UE5 (app.main)",
      "type": "python",
      "request": "launch",
      "module": "app.main",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [
        "--carla-version",
        "ue5"
      ]
    }
  ]
}
